background yes
font Liberation Sans Mono:size=8
use_xft yes
xftalpha 0.9
update_interval 0.5
total_run_times 0
own_window yes
own_window_type override
own_window_transparent no
own_window_colour 424242
own_window_hints undecorated,sticky,skip_taskbar,skip_pager 
double_buffer yes
draw_shades no
draw_outline no
draw_borders yes
draw_graph_borders yes
#default_color 162127
#default_color E7E7E7
default_color e1e0dd
alignment top_left
minimum_size 1273
gap_x 3
gap_y 3
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale yes

